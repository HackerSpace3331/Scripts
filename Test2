Tab:AddButton({
	Name = "Monster ESP",
	Callback = function()
         OrionLib:MakeNotification({
         Name = "Hacker Notification",
         Content = "Monster ESP has activated!",
         Image = "rbxassetid://16103628265",
         Time = 6.5
         })

         while wait(3) do
      		ESP:AddObjectListener(Workspace.Bunny.Events.Infected.ActiveNPCS, {
         Name = "Infected",
         CustomName = "Monster",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "mons" 
         })
         ESP.mons = true

         ESP:AddObjectListener(Workspace.Bunny.Events.Chase1Event, {
         Name = "RabbitMonster",
         CustomName = "Bonny",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "chase" 
         })
         ESP.chase = true

         ESP:AddObjectListener(Workspace.Bunny.Events.Chase2Event, {
         Name = "RabbitMonster",
         CustomName = "Bonny",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "chase2" 
         })
     
         ESP.chase2 = true 
         end
  	end    
})

Tab:AddButton({
	Name = "FOV (120)",
	Callback = function()
      		game.Workspace.Camera.FieldOfView = 120
  	end    
})

Tab:AddButton({
	Name = "Reset FOV",
	Callback = function()
      		game.Workspace.Camera.FieldOfView = 85
  	end    
})


local Section = Tab:AddSection({
	Name = "Player"
})


Tab:AddButton({
	Name = "Speedboost",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25
  	end    
})

Tab:AddButton({
	Name = "Reset Speedboost",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
  	end    
})


