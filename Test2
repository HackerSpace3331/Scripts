local OrionLib = loadstring(game:HttpGet(('https://pastebin.com/raw/WRUyYTdY')))()
local Window = OrionLib:MakeWindow({Name = "Hacker Hub | Piggy", HidePremium = false, IntroText = "Hacker Hub, Loading...", SaveConfig = true, ConfigFolder = "HackerHub RM"})

-- loadstring
local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

-- config
ESP.Boxes = false
ESP.Names = true
ESP:Toggle(true)


local Tab = Window:MakeTab({
Name = "Decay Chapter",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})


OrionLib:MakeNotification({
Name = "Hacker Notification",
Content = "Hacker Hub Made By Hacker3331",
Image = "rbxassetid://16103628265",
Time = 10
})


Tab:AddToggle({
	Name = "Player ESP",
	Default = false,
	Callback = function(Value)
		ESP.Players = Value
	end    
})

Tab:AddButton({
	Name = "Item ESP",
	Callback = function()
         OrionLib:MakeNotification({
         Name = "Hacker Notification",
         Content = "Item ESP has activated!",
         Image = "rbxassetid://16103628265",
         Time = 6.5
         })

         while wait(10) do
      		ESP:AddObjectListener(Workspace.ItemFolder, {
         Name = "Axe",
         CustomName = "Axe",
         Color = Color3.fromRGB(0, 255, 0), 
         IsEnabled = "axe" 
         })
