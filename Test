local OrionLib = loadstring(game:HttpGet(('https://pastebin.com/raw/WRUyYTdY')))()
local Window = OrionLib:MakeWindow({Name = "Hacker Hub | Residence Massacre [All Night], HidePremium = false, IntroText = "Hacker Hub, Loading...", SaveConfig = true, ConfigFolder = "HackerHub RM"})

-- loadstring
local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

-- config
ESP.Boxes = false
ESP.Names = true
ESP:Toggle(true)


local Tab = Window:MakeTab({
Name = "Night 1",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})


OrionLib:MakeNotification({
Name = "Hacker Notification",
Content = "Hacker Hub Made By Hacker3331",
Image = "rbxassetid://16103628265",
Time = 10
})


local Fuel = Tab:AddLabel("Fuel")

task.spawn(function()
        while wait() do
            pcall(function()
Fuel:Set("Fuel : ".. game:GetService("Workspace").Shack.Generator.Fuel.Value)
        end)
    end
end)


Tab:AddToggle({
	Name = "Player ESP",
	Default = false,
	Callback = function(Value)
		ESP.Players = Value
	end    
})

Tab:AddToggle({
	Name = "Infinite Stamina",
	Default = false,
	Callback = function(v)
		_G.InfStam = v
	end
})

spawn(function()
	while wait() do
		if _G.InfStam then
			game.Players.LocalPlayer.Character.Sprint.Overdrive.Value = 9999
		elseif not _G.InfStam then
			
		end
	end
end)

Tab:AddButton({
	Name = "Entity ESP (Include Halloween NPC)",
	Callback = function()
         OrionLib:MakeNotification({
         Name = "Hacker Notification",
         Content = "Entity ESP has activated!",
         Image = "rbxassetid://16103628265",
         Time = 6.5
         })

         while wait(3) do
      		ESP:AddObjectListener(Workspace, {
         Name = "Zombie",
         CustomName = "Zombie",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "zom" 
         })
         ESP.zom = true

         ESP:AddObjectListener(Workspace, {
         Name = "Mutant",
         CustomName = "Larry",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "entity" 
         })
         ESP.entity = true

         ESP:AddObjectListener(Workspace, {
         Name = "BloodZombie",
         CustomName = "Blood Zombie",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "blood" 
         })
     
         ESP.blood = true 
         end
  	end    
})

Tab:AddButton({
	Name = "Pumpkin ESP",
	Callback = function()
      		ESP:AddObjectListener(Workspace.Halloween.Pumpkins, {
         Name = "Pumpkin",
         CustomName = "Pumpkin",
         Color = Color3.fromRGB(213, 107, 0), 
         IsEnabled = "pumb" 
         })
         ESP.pumb = true

         OrionLib:MakeNotification({
         Name = "Hacker Notification",
         Content = "Pumpkin ESP has activated! be careful with the trick one..",
         Image = "rbxassetid://16103628265",
         Time = 6.5
         })
  	end    
})

Tab:AddButton({
	Name = "3rd View on House",
	Callback = function()
      		while wait() do
             game.Players.LocalPlayer.CameraMode = Enum.CameraMode.Classic
         end
  	end    
})

Tab:AddButton({
	Name = "Inf Battery (Equip Flashlight first)",
	Callback = function()
      		local plr = game.Players.LocalPlayer.Character
		
		      plr.Flashlight.Battery.Value = 999999
		      plr.Flashlight.Charges.Value = 999999
  	end    
})


local Tab2 = Window:MakeTab({
Name = "Night 2",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})


Tab2:AddToggle({
	Name = "Player ESP",
	Default = false,
	Callback = function(Value)
		ESP.Players = Value
	end    
})

Tab2:AddToggle({
	Name = "Infinite Stamina",
	Default = false,
	Callback = function(v)
		_G.InfStam = v
	end
})

spawn(function()
	while wait() do
		if _G.InfStam then
			game.Players.LocalPlayer.Character.Sprint.Overdrive.Value = 9999
		elseif not _G.InfStam then
			
		end
	end
end)

Tab2:AddButton({
	Name = "Entity ESP (Include Halloween NPC)",
	Callback = function()
         OrionLib:MakeNotification({
         Name = "Hacker Notification",
         Content = "Entity ESP has activated!",
         Image = "rbxassetid://16103628265",
         Time = 6.5
         })

         while wait(3) do
      		ESP:AddObjectListener(Workspace, {
         Name = "Zombie",
         CustomName = "Skeleton NPC",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "zom" 
         })
         ESP.zom = true

         ESP:AddObjectListener(Workspace, {
         Name = "Mutant",
         CustomName = "Larry Mutated",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "entity" 
         })
         ESP.entity = true

         ESP:AddObjectListener(Workspace, {
         Name = "Stalker",
         CustomName = "Stalker",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "stalker" 
         })

         ESP.stalker = true

         ESP:AddObjectListener(Workspace, {
         Name = "BloodZombie",
         CustomName = "Blood Skeleton",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "blood" 
         })
     
         ESP.blood = true 
         end
  	end    
})

Tab2:AddButton({
	Name = "Pumpkin ESP",
	Callback = function()
      		ESP:AddObjectListener(Workspace.Halloween.Pumpkins, {
