local Tab2 = Window:MakeTab({
Name = "Halloween Event ðŸŽƒ",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})


Tab2:AddToggle({
	Name = "ESP Player",
	Default = false,
	Callback = function(Value)
		ESP.Players = Value
	end    
})


local Section = Tab2:AddSection({
	Name = "Minigame 1"
})


Tab2:AddButton({
	Name = "ESP All",
	Callback = function()
      		ESP:AddObjectListener(Workspace.Puzzle.Pumpkins, {
         Name = "pumpkin",
         CustomName = "Pumbkin",
         Color = Color3.fromRGB(0, 255, 0), 
         IsEnabled = "pumb" 
         })
         ESP.pumb = true

         ESP:AddObjectListener(Workspace.NPCS, {
         Name = "Entity",
         CustomName = "Monster",
         Color = Color3.fromRGB(255, 0, 0), 
         IsEnabled = "monster" 
         })
         ESP.monster = true
  	end    
})

Tab2:AddButton({
	Name = "Teleport to Exit Room (If you alr collect all pumbkin)",
	Callback = function()
      		local rootpart = game.Players.LocalPlayer.Character.HumanoidRootPart
         rootpart.CFrame = CFrame.new(-49.3997192, 5.55001831, 162.899963, 1, 0, 0, 0, 1, 0, 0, 0, 1)
  	end    
})


local Section = Tab2:AddSection({
	Name = "Minigame 2"
})


Tab2:AddButton({
	Name = "Teleport to exit elevator (Minigame 2)",
	Callback = function()
      		local rootpart = game.Players.LocalPlayer.Character.HumanoidRootPart
         rootpart.CFrame = game:GetService("Workspace").ExitElevator.Invisible.CFrame
  	end    
})

